<div class="container mt-5">
  <h1 class="mb-4">My Cart</h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Product</th>
        <th scope="col">Price</th>
        <th scope="col">Quantity</th>

      </tr>
    </thead>
    <tbody>
      <tr>
         {{#each books as |book|}}
        <td>{{book.book_name}}</td>
        <td>{{book.price}}</td>
        <td>1</td>

      </tr>

    </tbody>
  </table>
 